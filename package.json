{
    "name": "root",
    "private": true,
    "scripts": {
        "bootstrap": "lerna bootstrap",
        "start": "yarn electron:start",
        "start:dev": "lerna run start:dev",
        "package": "yarn package:build && yarn electron:package",
        "package:build": "yarn common:build && yarn iracing-sdk:build && yarn server:iracing:build && yarn server:web:build && yarn electron:build",
        "client:build": "cd ./packages/evo-client && yarn build",
        "client:start": "cd ./packages/evo-client && yarn start",
        "client:start:dev": "cd ./packages/evo-client && yarn start:dev",
        "common:build": "cd ./packages/evo-common && yarn build",
        "server:iracing:start": "cd ./packages/evo-iracing-server && yarn start",
        "server:iracing:start:dev": "cd ./packages/evo-iracing-server && yarn start:dev",
        "server:iracing:stop:dev": "cd ./packages/evo-iracing-server && yarn stop:dev",
        "server:iracing:build": "cd ./packages/evo-iracing-server && yarn build",
        "server:web:build": "cd ./packages/evo-web-server && yarn build",
        "electron:start": "cd ./packages/evo-electron && yarn start",
        "electron:build": "cd ./packages/evo-electron && yarn build",
        "electron:package": "cd ./packages/evo-electron && yarn package",
        "iracing-sdk:build": "cd ./packages/evo-iracing-sdk && yarn build",
        "iracing-sdk:build:dev": "cd ./packages/evo-iracing-sdk && yarn build:dev",
        "format": "./node_modules/.bin/prettier --write \"packages/**/*.{js,jsx,json,css,md}\"",
        "test:lint": "eslint \"packages/**/*.{js,jsx}\"",
        "test:types": "./node_modules/.bin/flow"
